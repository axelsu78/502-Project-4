{
   "version": "2.0.0",
   "tasks": [
      {
         "type": "process",
         "label": "g++ build movie test",
         "command": "/usr/bin/g++",
         "args": [
            "-std=c++17",
            "-Wall",
            "-Wextra",
            "-g",
            "movie.cpp",
            "classicmovie.cpp",
            "comedymovie.cpp",
            "dramamovie.cpp",
            "movie_test.cpp",
            "-o",
            "movie_test"
         ],
         "options": {
            "cwd": "${workspaceFolder}"
         },
         "problemMatcher": [
            "$gcc"
         ],
         "group": {
            "kind": "build",
            "isDefault": true
         }
      },
      {
         "type": "process",
         "label": "Run Movie Test",
         "command": "${workspaceFolder}/movie_test",
         "options": {
            "cwd": "${workspaceFolder}"
         },
         "problemMatcher": [],
         "group": {
            "kind": "test",
            "isDefault": false
         }
      }
   ]
}